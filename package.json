            "vitest": "^4.0.16",
            "wait-on": "^9.0.3"
      },
      "main": "dist-electron/main.js",
      "scripts": {
            "dev": "vite",
            "lint": "eslint .",
            "build": "tsc && vite build",
            "preview": "vite preview",
            "ts:electron": "tsc -p tsconfig.electron.json",
            "electron:dev": "concurrently -k \"cross-env BROWSER=none npm run dev\" \"wait-on http://localhost:3000 && npm run ts:electron && cross-env VITE_DEV_SERVER_URL=http://localhost:3000 electron .\"",
            "electron:build": "npm run build && npm run ts:electron && electron-builder",
            "test": "vitest"
      },
      "build": {
            "appId": "com.auroraos.app",
            "productName": "Aurora OS",
            "directories": {
                  "output": "release"
            },
            "files": [
                  "dist/**/*",
                  "dist-electron/**/*"
            ],
            "win": {
                  "target": [
                        "nsis",
                        "portable"
                  ],
                  "icon": "public/vite.svg"
            },
            "mac": {
                  "target": "dmg",
                  "icon": "public/vite.svg",
                  "hardenedRuntime": true,
                  "gatekeeperAssess": false
            }
      }
}
