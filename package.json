      "devDependencies": {
            "@eslint/js": "^9.39.2",
            "@testing-library/dom": "^10.4.1",
            "@testing-library/jest-dom": "^6.9.1",
            "@testing-library/react": "^16.3.2",
            "@types/howler": "^2.2.12",
            "@types/node": "^25.1.0",
            "@types/prismjs": "^1.26.5",
            "@types/react": "^19.2.13",
            "@types/react-dom": "^19.2.3",
            "@vitejs/plugin-react-swc": "^4.2.3",
            "concurrently": "^9.2.1",
            "cross-env": "^10.1.0",
            "electron": "^40.2.1",
            "electron-builder": "^26.4.0",
            "eslint": "^9.39.2",
            "eslint-plugin-react-hooks": "^7.0.1",
            "eslint-plugin-react-refresh": "^0.4.26",
            "globals": "^17.2.0",
            "jsdom": "^28.0.0",
            "postcss": "^8.5.6",
            "tailwindcss": "^4.1.17",
            "tailwindcss-animate": "^1.0.7",
            "tsx": "^4.21.0",
            "typescript": "^5.9.3",
            "typescript-eslint": "^8.55.0",
            "vite": "^7.3.1",
            "vite-plugin-compression2": "^2.4.0",
            "vitest": "^4.0.18",
            "wait-on": "^9.0.4"
      },
      "main": "dist-electron/main.js",
      "scripts": {
            "dev": "vite",
            "lint": "eslint .",
            "build": "tsc && vite build",
            "preview": "vite preview",
            "ts:electron": "tsc -p tsconfig.electron.json && npm run copy:electron-assets",
            "copy:electron-assets": "node -e \"require('fs').cpSync('electron/splash.html','dist-electron/splash.html')\"",
            "electron:dev": "concurrently -k \"cross-env BROWSER=none npm run dev\" \"wait-on http://localhost:3000 && npm run ts:electron && cross-env VITE_DEV_SERVER_URL=http://localhost:3000 electron .\"",
            "electron:build": "npm run build && npm run ts:electron && electron-builder",
            "electron:build:all": "npm run build && npm run ts:electron && electron-builder -mwl",
            "test": "vitest",
            "translations:generate": "tsx scripts/ts-to-json.ts",
            "translations:validate": "tsx scripts/validate-translations.ts",
            "translations:build": "npm run translations:validate && tsx scripts/json-to-ts.ts",
            "translations:build:changed": "npm run translations:validate && tsx scripts/json-to-ts.ts --changed"
      },
      "build": {
            "appId": "com.auroraos.app",
            "productName": "Aurora OS.js",
            "asar": true,
            "compression": "maximum",
            "copyright": "Copyright ©2026 Dope Pixels & MOONHOUND Studio",
            "electronFuses": {
                  "runAsNode": false,
                  "enableCookieEncryption": true,
                  "enableNodeOptionsEnvironmentVariable": false,
                  "enableNodeCliInspectArguments": false,
                  "enableEmbeddedAsarIntegrityValidation": true,
                  "onlyLoadAppFromAsar": true
            },
            "publish": {
                  "provider": "github",
                  "owner": "mental-os",
                  "repo": "Aurora-OS.js",
                  "timeout": 300000
            },
            "directories": {
                  "output": "release"
            },
            "files": [
                  "dist/**/*",
                  "dist-electron/**/*"
            ],
            "win": {
                  "target": [
                        "nsis",
                        "portable"
                  ],
                  "icon": "public/icon.ico",
                  "verifyUpdateCodeSignature": false,
                  "artifactName": "${productName}-${version}-${os}-${arch}-setup.${ext}"
            },
            "nsis": {
                  "artifactName": "${productName}-${version}-${os}-${arch}-installer.${ext}",
                  "menuCategory": "Dope Pixels",
                  "oneClick": false,
                  "allowToChangeInstallationDirectory": true,
                  "perMachine": false,
                  "deleteAppDataOnUninstall": true,
                  "displayLanguageSelector": true,
                  "installerIcon": "public/icon.ico",
                  "uninstallerIcon": "public/icon.ico",
                  "uninstallDisplayName": "Aurora OS.js",
                  "createDesktopShortcut": true,
                  "createStartMenuShortcut": true
            },
            "portable": {
                  "artifactName": "${productName}-${version}-${os}-${arch}-portable.${ext}"
            },
            "mac": {
                  "target": [
                        "dmg",
                        "zip"
                  ],
                  "icon": "public/icon.icns",
                  "hardenedRuntime": true,
                  "gatekeeperAssess": false,
                  "identity": "-",
                  "darkModeSupport": true,
                  "category": "public.app-category.simulation-games",
                  "artifactName": "${productName}-${version}-${os}-${arch}.${ext}"
            },
            "dmg": {
                  "background": "public/dmg-background.png",
                  "window": {
                        "width": 540,
                        "height": 380
                  },
                  "icon": "public/icon.icns",
                  "iconSize": 80,
                  "title": "${productName} ${version}",
                  "format": "ULFO",
                  "contents": [
                        {
                              "x": 130,
                              "y": 220
                        },
                        {
                              "x": 410,
                              "y": 220,
                              "type": "link",
                              "path": "/Applications"
                        }
                  ]
            },
            "linux": {
                  "target": [
                        "AppImage",
                        "deb"
                  ],
                  "maintainer": "Cătălin-Robert Drăgoiu <mentalos@proton.me>",
                  "icon": "public/icon.png",
                  "category": "Game",
                  "synopsis": "Hacking simulator game",
                  "description": "A high-fidelity OS hacking simulator set in the mental.os() universe.",
                  "artifactName": "${productName}-${version}-${os}-${arch}.${ext}"
            },
            "electronLanguages": [
                  "en",
                  "de",
                  "es",
                  "fr",
                  "pt",
                  "ro",
                  "zh",
                  "ru",
                  "ja",
                  "pl",
                  "ko",
                  "tr",
                  "hi"
            ]
      },
      "overrides": {
            "react-day-picker": {
                  "react": "^19.0.0",
                  "react-dom": "^19.0.0"
            },
            "tar": "^7.5.6"
      }
}
